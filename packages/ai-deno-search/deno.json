{
  "name": "@mcampa/ai-deno-search",
  "version": "0.0.1",
  "exports": "./mod.ts",
  "license": "MIT",
  "compilerOptions": {
    "lib": ["deno.window", "es2022"],
    "strict": true
  },
  "imports": {
    "openai": "npm:openai@^5.1.1",
    "voyageai": "npm:voyageai@^0.0.4",
    "@google/genai": "npm:@google/genai@^1.9.0",
    "ollama": "npm:ollama@^0.5.16"
  },
  "tasks": {
    "test": "deno test --allow-read --allow-net --allow-env",
    "check": "deno check mod.ts",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "fmt:check": "deno fmt --check",
    "example": "deno run --allow-env --allow-net examples/basic-search.ts"
  },
  "fmt": {
    "useTabs": false,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": false,
    "proseWrap": "preserve",
    "include": ["src/**/*.ts"]
  },
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "exclude": ["no-explicit-any", "require-await"]
    },
    "include": ["src/**/*.ts"]
  }
}
